  "version": "0.2.0",
  "configurations": [
    {
      "type": "cortex-debug",
      "request": "launch",
      "name": "My Debug J-Link",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/STM32F103C6.elf",
      "serverpath": "C:/Program Files (x86)/SEGGER/JLink/JLinkGDBServerCL.exe",
      "servertype": "jlink",
      "device": "STM32F103C6",
      "interface": "swd",
      "serialNumber": "",
      //"preLaunchTask": "cargo build",
      "runToEntryPoint": "main",
      "svdFile": "${workspaceRoot}/STM32F103xx.svd",
      "swoConfig": {
        "enabled": true,
        "cpuFrequency": 8000000,
        "swoFrequency": 2000000,
        "source": "probe",
        "decoders": [
            { "type": "console", "label": "ITM", "port": 0 }
        ]
      }
    }
  ]
}
